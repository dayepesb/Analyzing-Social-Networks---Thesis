package co.edu.poli.Node;

import java.awt.Color;
import java.util.HashMap;

public interface Node {

	int idNode;
	HashMap<String, String> properties;
	String label;
	Color color;

	// getters
	public abstract int getIdNode();

	public abstract HashMap<String, String> getProperties();

	public abstract String getLabel();

	public abstract Color getColor();

	// setters
	public abstract void setIdNode(int idNode);

	public abstract void setProperties(HashMap<String, String> properties);

	public abstract void setLabel(String label);

	public abstract void setColor(Color color);

}
